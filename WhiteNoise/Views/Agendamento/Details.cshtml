@model WhiteNoise.Models.Agendamento.AgendamentoGridModel

<div>
    <h2>Detalhes do Agendamento</h2>
    <div>
        <dl class="row">
            <dt class="col-sm-2">Data</dt>
            <dd class="col-sm-10">@Model.DataHora</dd>

            <dt class="col-sm-2">Tipo de Agendamento</dt>
            <dd class="col-sm-10">@Model.Tipo</dd>

            <dt class="col-sm-2">Status do Agendamento</dt>
            <dd class="col-sm-10">@Model.Status</dd>

            <dt class="col-sm-2">Observação</dt>
            <dd class="col-sm-10">@Model.Observacoes</dd>

            <dt class="col-sm-2">Paciente</dt>
            <dd class="col-sm-10">@Model.Paciente</dd>

            <dt class="col-sm-2">Profissional</dt>
            <dd class="col-sm-10">@Model.Profissional</dd>
        </dl>

        <a asp-action="Edit" asp-route-id="@Model.Id" class="btn btn-primary">Editar</a>
        <a asp-action="Index" class="btn btn-secondary">Voltar</a>
    </div>
</div>